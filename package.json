{
  "name": "dxf",
  "version": "3.6.0",
  "description": "N9 DXF parser for node/browser, cloned from git://github.com/bjnortier/dxf.git",
  "main": "lib/index.js",
  "bin": {
    "dxf-to-svg": "lib/cli.js"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "lint": "standard",
    "test:unit": "mocha --require @babel/register --recursive test/unit/**",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:functional": "cd test/functional && webpack-dev-server",
    "test": "npm run lint && npm run test:unit",
    "compile": "babel --optional runtime -d lib/ src/",
    "dist": "mkdir -p dist && browserify lib/index.js --standalone dxf -o dist/dxf.js",
    "prepublishOnly": "rimraf dist/ lib/ && npm run compile && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bjnortier/dxf.git"
  },
  "keywords": [
    "DXF",
    "Parser",
    "SVG"
  ],
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/nimbus9inc/dxf"
  },
  "homepage": "https://bitbucket.org/nimbus9inc/dxf",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "browserify": "^16.2.3",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "raw-loader": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "lodash.clonedeep": "^4.5.0",
    "pretty-data": "^0.40.0",
    "round10": "^1.0.3",
    "vecks": "^3.7.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after"
    ],
    "ignore": [
      "lib/",
      "dist/"
    ]
  }
}
